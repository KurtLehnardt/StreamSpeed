{
  "manifest_version": 2,

  "name": "StreamSpeed",
  "description": "Fine-tune video speed of popular streaming services. Use arrow keys to adjust, backspace to reset.",
  "version": "1.3",

  "browser_action": {
   "default_icon": {
      "19": "images/play19.png",
      "38": "images/play38.png"
   },
   "default_popup": "popup.html",
   "default_title": "StreamSpeed"
  },
  "icons": { 
    "16": "images/play16.png",
    "48": "images/play48.png",
    "128": "images/play128.png"
  },
  "permissions": [
    "activeTab"
   ],
  "content_security_policy": "script-src 'self' https://www.youtube.com; object-src 'self'"
}
